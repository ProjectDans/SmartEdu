<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/menu1"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="167dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="325dp"
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/descWelcome1"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="246dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:id="@+id/descWelcome2"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="246dp" />
        <Constraint
            android:layout_height="338dp"
            android:layout_marginTop="89dp"
            android:layout_width="337dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:id="@+id/imageWelcome2"
            motion:layout_constraintStart_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/descWelcome1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="325dp"
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="246dp" />
        <Constraint
            android:id="@+id/imageWelcome1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="337dp"
            android:layout_height="338dp"
            android:layout_marginEnd="47dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:layout_marginStart="48dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="89dp" />
        <Constraint
            android:id="@+id/menu1"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="167dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:id="@+id/descWelcome2"
            motion:layout_constraintStart_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="246dp" />
        <Constraint
            android:layout_height="338dp"
            android:layout_marginTop="89dp"
            android:layout_width="337dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:id="@+id/imageWelcome2"
            motion:layout_constraintStart_toEndOf="parent" />
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintStart_toEndOf="@+id/menu1"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="14dp"
            android:layout_marginTop="80dp" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
    </ConstraintSet>
    <ConstraintSet
        android:id="@+id/welcome2">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="325dp"
            android:id="@+id/descWelcome2"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="246dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/descWelcome1"
            android:layout_width="325dp"
            android:layout_height="80dp"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteY="227dp"
            android:layout_marginTop="246dp" />
        <Constraint
            android:id="@+id/menu1"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="167dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintStart_toEndOf="@+id/menu1"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="14dp"
            android:layout_marginTop="80dp" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:layout_height="338dp"
            android:layout_marginTop="89dp"
            android:layout_width="337dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:id="@+id/imageWelcome1"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageWelcome2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="337dp"
            android:layout_height="338dp"
            android:layout_marginEnd="47dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:layout_marginStart="48dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="89dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            motion:layout_constraintStart_toEndOf="parent"
            android:id="@+id/descWelcome3"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteY="246dp"
            android:layout_marginTop="246dp" />
        <Constraint
            android:layout_height="338dp"
            android:layout_marginTop="89dp"
            android:layout_width="337dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:id="@+id/imageWelcome3"
            motion:layout_constraintStart_toEndOf="parent" />
        <Constraint
            android:id="@+id/menu3"
            motion:layout_constraintStart_toEndOf="@+id/menu2"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="14dp"
            android:layout_marginTop="80dp" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            motion:layout_editor_absoluteY="785dp"
            android:layout_height="50dp"
            android:layout_width="275dp"
            android:id="@+id/btnNext"
            motion:layout_constraintStart_toEndOf="parent" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/welcome2" >
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>
    <ConstraintSet
        android:id="@+id/welcome3"
        motion:deriveConstraintsFrom="@+id/welcome2" >
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="246dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="325dp"
            android:id="@+id/descWelcome2"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="246dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="0dp"
            android:id="@+id/descWelcome3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/menu3"
            motion:layout_constraintStart_toEndOf="@+id/menu2"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="14dp"
            android:layout_marginTop="80dp" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintStart_toEndOf="@+id/menu1"
            android:layout_width="10dp"
            android:layout_height="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/descWelcome1"
            android:layout_marginStart="14dp"
            android:layout_marginTop="80dp" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:layout_height="338dp"
            android:layout_marginTop="88dp"
            android:layout_width="337dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:id="@+id/imageWelcome2"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="338dp"
            android:layout_marginTop="29dp"
            android:layout_width="337dp"
            motion:layout_constraintTop_toBottomOf="@+id/menu1"
            android:id="@+id/imageWelcome3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/buttonNext"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="275dp"
            android:layout_height="50dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            android:alpha="1"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/welcome2"
        motion:constraintSetEnd="@+id/welcome3" >
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>
</MotionScene>